# start command
/usr/lib/jvm/java-1.8.0-openjdk-1.8.0.31-2.b13.el7.x86_64/jre/bin/java -Xmx512m -XX:MaxPermSize=256m -Xms128m -XX:PermSize=48m -Dbaseurl=https://localhost.localdomain:8443/stash -Dplugin.root.directories=/home/vagrant/stash/amps-standalone -Dcargo.servlet.uriencoding=UTF-8 -Dplugin.resource.directories= -Datlassian.dev.mode=true -Dstash.home=/home/vagrant/stash/amps-standalone/target/stash/home -Djava.awt.headless=true -Dbaseurl.display=https://localhost:8443/stash -Datlassian.sdk.version=5.0.13 -Dcatalina.home=/home/vagrant/stash/amps-standalone/target/container/tomcat7x/apache-tomcat-7.0.52 -Dcatalina.base=/home/vagrant/stash/amps-standalone/target/container/tomcat7x/cargo-stash-home -Djava.io.tmpdir=/home/vagrant/stash/amps-standalone/target/container/tomcat7x/cargo-stash-home/temp -Djava.util.logging.manager=org.apache.juli.ClassLoaderLogManager -Djava.util.logging.config.file=/home/vagrant/stash/amps-standalone/target/container/tomcat7x/cargo-stash-home/conf/logging.properties -classpath /home/vagrant/stash/amps-standalone/target/container/tomcat7x/apache-tomcat-7.0.52/bin/tomcat-juli.jar:/home/vagrant/stash/amps-standalone/target/container/tomcat7x/apache-tomcat-7.0.52/bin/bootstrap.jar:/usr/lib/jvm/java-1.8.0-openjdk-1.8.0.31-2.b13.el7.x86_64/lib/tools.jar org.apache.catalina.startup.Bootstrap start

# keytool command
keytool -genkey -alias tomcat -keyalg RSA -dname "cn=localhost, ou=IT, o=Awesome, c=US" -storepass changeit -keypass changeit -noprompt
